{
    "name": "@valapi/discord-bot",
    "version": "1.0.0",
    "description": "Valorant Discord Bot By ING3_",
    "main": "dist/index.js",
    "type": "commonjs",
    "engines": {
        "node": "16.x",
        "npm": "8.x"
    },
    "scripts": {
        "types": "npx tsc --project tsconfig.json",
        "format": "prettier --config ./.prettierrc --ignore-path ./.prettierignore --write .",
        "lint": "npx eslint src/**",
        "build": "npm run format && npm run types",
        "start": "npm run types && nodemon --ignore '/*' ./dist/index.js",
        "test": "ts-node src/index.ts --typeCheck --logError"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/valapi/discord-bot.git"
    },
    "keywords": [
        "Valorant",
        "Discord",
        "Bot"
    ],
    "author": {
        "name": "ing3kth",
        "url": "https://github.com/KTNG-3/"
    },
    "contributors": [
        {
            "name": "ING3_",
            "url": "https://github.com/KTNG-3/"
        }
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/valapi/discord-bot/issues"
    },
    "homepage": "https://github.com/valapi/discord-bot#readme",
    "dependencies": {
        "@ing3kth/core": "^2.0.2",
        "crypto-js": "^4.1.1",
        "discord-api-types": "^0.37.18",
        "discord.js": "^14.6.0",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "mongoose": "^6.7.2",
        "nodemon": "^2.0.20",
        "valorant.ts": "^2.0.1"
    },
    "devDependencies": {
        "@types/crypto-js": "^4.1.1",
        "@types/express": "^4.17.14",
        "@types/node": "^18.11.9",
        "@types/nodemon": "^1.19.2",
        "@types/tough-cookie": "^4.0.2",
        "@typescript-eslint/eslint-plugin": "^5.43.0",
        "@typescript-eslint/parser": "^5.43.0",
        "eslint": "^8.28.0",
        "tslib": "^2.4.1",
        "typescript": "^4.9.3"
    }
}